{
    "name": "miles/slim-test",
    "description": "Slim Testing helper",
    "type": "library",
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.5",
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Miles\\Slim\\Test\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Miles\\Slim\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "php vendor/bin/phpcs",
            "php vendor/bin/phpunit"
        ]
    },
    "license": "MIT License",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ]
}
