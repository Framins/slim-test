{
    "name": "framins/slim-test",
    "description": "Slim Testing helper",
    "type": "library",
    "keywords": [
        "slim",
        "testing"
    ],
    "license": "MIT License",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/database": "~5.2.45",
        "phpunit/phpunit": "^4.0",
        "slim/slim": "^3.4"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Framins\\Slim\\Test\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Framins\\Slim\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "php vendor/bin/phpcs",
            "php vendor/bin/phpunit",
            "php vendor/bin/behat"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
